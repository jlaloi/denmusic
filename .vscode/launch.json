{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "outputCapture": "std",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--importmap=./import_map.json",
        "--allow-net",
        "--allow-read",
        "--allow-write",
        "--inspect",
        "server.ts"
      ],
      "port": 9229
    }
  ]
}
